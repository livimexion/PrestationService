@model IEnumerable<PrestationService.Models.Transfert>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.operateur)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tel)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.operateur)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tel)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idCash }) |
            @Html.ActionLink("Details", "Details", new { id=item.idCash }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idCash })
        </td>
    </tr>
}

</table>
